"workbench.colorCustomizations": {
	"editor.background": "#20201C",
	"editorCursor.foreground": "#FFD",
	"editor.foreground": "#C0A088",
	"editor.selectionBackground": "#504030"
},

"editor.tokenColorCustomizations": {
	"variables": "#C0A088",
	"functions": "#CA7",
	"types": "#D65",
	"comments": "#98b850",
	"strings": "#D73",
	"keywords": "#B63",
	"numbers": "#CC8",
	"textMateRules": [
		{"scope": "punctuation","settings": {"foreground": "#C0A088"}},
		{"scope": "keyword.operator","settings": {"foreground": "#C0A088"}},
		{"scope": "keyword.operator.sizeof","settings": {"foreground": "#C0A088"}},
		{"scope": "keyword.other.unit","settings": {"foreground": "#CC8"}},
		{"scope": "constant.language","settings": {"foreground": "#CC8"}},
		{"scope": "storage.type","settings": {"foreground": "#D65"}},
		{"scope": "storage.modifier","settings": {"foreground": "#E64"}},
		{"scope": "entity.name.function.preprocessor","settings": {"foreground": "#865"}},
	]
},

"editor.semanticTokenColorCustomizations": {
	"rules": {
		"enumMember": "#CC8",
		"macro": "#C47",
		"namespace": "#D87",
		"operator": "#ECA",
	}
},